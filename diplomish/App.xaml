<Application x:Class="diplomish.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:diplomish"
             StartupUri="windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="nigger" UriSource="../resoursess/anon.jpg"></BitmapImage>
            <FontFamily x:Key="yakuza" >resoursess/edosz.ttf</FontFamily>
            <Style TargetType="Button">
                <Setter Property="Background" Value="#3dccc7"></Setter>
            </Style>
            <Style TargetType="TextBox">
                <Setter Property="Background" Value="#3dccc7"></Setter>
            </Style>
            <Style TargetType="Label">
                <Setter Property="FontFamily" Value="/diplomish;component/rez/#Leto Text Sans Defect"></Setter>
                <Setter Property="FontSize" Value="50"></Setter>
            </Style>
        </ResourceDictionary>


    </Application.Resources>

</Application>
